<%= render "layouts/error_message" %>

<%= form_with model: @project, local: true do |form| %>
  <div class="form-group mb-4">
    <%= form.label :name %>
    <%= form.text_field :name, class: 'form-control', autocomplete: 'off' %>
  </div>

  <div class="form-group mb-4">
    <%= form.label :description%>
    <%= form.text_area :description, class: 'form-control', autocomplete: 'off' %>
  </div>

  <div class="actions">
    <%= form.submit class: 'btn btn-primary form-control' %>
  </div>
<% end %>
