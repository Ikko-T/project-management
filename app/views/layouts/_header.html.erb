<% if user_signed_in? %>
  <%= link_to 'Log Out', destroy_user_session_path, method: :delete, data: { confirm: "Do you want to log out?" } %>
  <%= "#{ current_user.email }" %>
<% else %>
  <%= link_to 'Log In', new_user_session_path %>
  <%= link_to 'Sign Up', new_user_registration_path %>
<% end %>
